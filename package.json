{
  "name": "@proximaone/stream-client-js",
  "version": "0.3.0",
  "description": "Client to Proxima StreamDB",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "lib"
  ],
  "author": "proxima.one",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf -rf dist && rimraf tsconfig.tsbuildinfo && rimraf coverage",
    "format": "prettier --write \"{src,test}/**/*.ts\" --double-quote --trailing-comma es5",
    "lint": "eslint . --ext .ts",
    "copy-content-files": "copyfiles -u 1 src/**/*.d.ts src/**/*.proto src/**/*.js dist/",
    "compile": "tsc --build",
    "prebuild": "yarn clean && yarn format && yarn lint && echo Using TypeScript && tsc --version",
    "build": "yarn compile && yarn copy-content-files",
    "prepublishOnly": "yarn build",
    "push": "buf push ./src/proto",
    "test": "jest \".*\\.(spec|test)\\.ts$\" --coverage",
    "test:e2e": "jest \"test/.*\\.e2e\\.ts$\" --coverage --forceExit --runInBand",
    "pub": "yarn publish"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/fs-extra": "^9.0.11",
    "@types/jest": "^27.0.2",
    "@types/js-yaml": "^4.0.1",
    "@types/lodash": "^4.14.168",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "copyfiles": "^2.4.1",
    "eslint": "7.24.0",
    "fs-extra": "^9.1.0",
    "jest": "^27.3.1",
    "js-yaml": "^4.1.0",
    "loadash": "^1.0.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.3.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.4.2",
    "debug": "^4.3.2",
    "dotenv": "^10.0.0",
    "google-protobuf": "^3.19.0",
    "grpc": "^1.24.11",
    "rxjs": "^7.1.0"
  }
}
