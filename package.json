{
  "name": "@proximaone/stream-client-js",
  "version": "0.1.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "prepack": "yarn build",
    "dev": "ts-node src/index.ts",
    "start": "yarn start:server",
    "start:client": "ts-node -r tsconfig-paths/register src/client.ts",
    "build": "yarn clean && yarn compile && yarn copy-content-files",
    "clean": "rimraf -rf dist && rimraf tsconfig.tsbuildinfo",
    "copy-content-files": "copyfiles -u 1 src/**/*.proto dist/ && copyfiles -u 1 src/**/*.js dist/",
    "compile": "tsc --build",
    "lint": "yarn run eslint",
    "test": "yarn jest \".*\\.(spec|test)\\.ts$\" --coverage",
    "test:e2e": "yarn jest \"test/.*\\.e2e\\.ts$\" --coverage --forceExit --runInBand",
    "pub": "yarn publish",
    "docker:build": "./deployment/build.sh",
    "docker:publish": "./deployment/publish.sh",
    "docker:dev:run": "./deployment/runProcess.sh",
    "docker:dev:buildrun": "yarn docker:build && yarn docker:run"
  },
  "author": "proxima.one",
  "license": "MIT",
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^27.0.2",
    "@types/fs-extra": "^9.0.11",
    "@types/js-yaml": "^4.0.1",
    "@types/lodash": "^4.14.168",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "tsconfig-paths": "^3.9.0",
    "copyfiles": "^2.4.1",
    "eslint": "7.24.0",
    "jest": "^27.3.1",
    "loadash": "^1.0.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.3.1",
    "typescript": "^4.4.4",
    "fs-extra": "^9.1.0",
    "js-yaml": "^4.1.0"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.4.2",
    "commander": "^8.3.0",
    "debug": "^4.3.2",
    "dotenv": "^10.0.0",
    "google-protobuf": "^3.19.0",
    "grpc": "^1.24.11",
    "rxjs": "^7.1.0",
    "web3": "^1.5.2",
    "lodash": "^4.17.21"
  }
}
