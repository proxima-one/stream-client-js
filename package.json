{
  "name": "@proximaone/stream-client-js",
  "version": "1.1.2",
  "description": "Client to Proxima StreamDB",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build",
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "proxima.one",
  "license": "MIT",
  "scripts": {
    "prepack": "yarn build",
    "clean": "rimraf -rf build && rimraf tsconfig.tsbuildinfo && rimraf coverage",
    "format": "prettier --write \"{src,__tests__}/**/*.ts\"",
    "lint": "eslint . --ext .ts",
    "copy-content-files": "copyfiles -u 1 src/**/*.d.ts src/**/*.proto src/**/*.js build/",
    "compile": "tsc --build",
    "prebuild": "yarn clean && yarn format && yarn lint && echo Using TypeScript && tsc --version",
    "build": "yarn compile && yarn copy-content-files",
    "codegen": "buf generate",
    "dev": "ts-node ./src/dev.ts",
    "test": "jest \".*\\.(spec|test)\\.ts$\" --coverage",
    "test:e2e": "jest \".*\\.(e2e)\\.ts$\" --coverage",
    "pub": "lerna publish --no-verify-access",
    "samples:eth_block_parsing": "ts-node ./samples/ethereum_block_parsing.ts"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/fs-extra": "^9.0.11",
    "@types/google-protobuf": "^3.15.5",
    "@types/jest": "^27.0.2",
    "@types/js-yaml": "^4.0.1",
    "@types/lodash": "^4.14.168",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "copyfiles": "^2.4.1",
    "eslint": "7.24.0",
    "fs-extra": "^9.1.0",
    "grpc-tools": "^1.11.2",
    "grpc_tools_node_protoc_ts": "^5.3.2",
    "jest": "^27.3.1",
    "js-yaml": "^4.1.0",
    "lerna": "^4.0.0",
    "loadash": "^1.0.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.3.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.6.1",
    "axios": "^1.1.3",
    "google-protobuf": "^3.20.0",
    "lodash": "^4.17.21",
    "node-cache": "^5.1.2",
    "rxjs": "^7.5.5"
  }
}
